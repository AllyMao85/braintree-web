{
  "env": {
    "es6": true,
    "jest": true,
    "node": true,
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "globals":{
    "global": true,
    "triggerEvent": false,
    "jest": true

  },
  "rules": {
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": false,
        "objectsInObjects": false
      }
    ]
  }
}
